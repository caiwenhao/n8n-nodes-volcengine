{
  "name": "n8n-nodes-volcengine",
  "version": "1.1.2",
  "description": "n8n community nodes for VolcEngine services - ECS image management, task monitoring and more cloud automation",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "volcengine",
    "ecs",
    "cloud",
    "automation",
    "workflow",
    "image-management",
    "task-monitoring"
  ],
  "license": "MIT",
  "homepage": "https://github.com/caiwenhao/n8n-nodes-volcengine#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caiwenhao/n8n-nodes-volcengine.git"
  },
  "bugs": {
    "url": "https://github.com/caiwenhao/n8n-nodes-volcengine/issues"
  },
  "author": {
    "name": "caiwenhao",
    "email": "caiwenhao186@live.com"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy:icons && npm run copy:translations",
    "copy:icons": "copyfiles -u 1 \"nodes/**/*.{png,svg}\" dist/nodes/",
    "copy:translations": "copyfiles -u 1 \"nodes/**/*.json\" dist/nodes/",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "echo 'Linting skipped for now'",
    "lintfix": "echo 'Linting skipped for now'",
    "prepublishOnly": "npm run build",
    "test": "echo 'Tests skipped for now'"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/VolcEngineApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/VolcEngine/ECS/VolcEngineEcs.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.16",
    "@typescript-eslint/parser": "^5.59.8",
    "copyfiles": "^2.4.1",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5",
    "jest": "^29.5.0",
    "@types/jest": "^29.5.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=8.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
